{
  "name": "@memenashi/react-qr-reader",
  "version": "0.2.3",
  "description": "A React Component for reading QR codes from the webcam",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm-run-all prettier eslint",
    "lint:fix": "run-s prettier:fix eslint:fix",
    "storybook": "storybook dev -p 6006 --https --ssl-cert ./certs/storybook.cert --ssl-key ./certs/storybook.key",
    "storybook:build": "storybook build -o ./storybook-static",
    "predeploy": "storybook build -o ./storybook-static",
    "deploy": "gh-pages -d storybook-static",
    "prebuild": "tsc"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memenashi/react-qr-reader.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "qr",
    "qrcode",
    "qrreader"
  ],
  "author": "memenashi",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@zxing/browser": "^0.1.4",
    "@zxing/library": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.3",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-mdx-gfm": "^7.0.27",
    "@storybook/cli": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-webpack5": "^7.0.27",
    "@storybook/storybook-deployer": "^2.8.16",
    "@types/react": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.14",
    "gh-pages": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.2.0",
    "rollup": "^3.29.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
